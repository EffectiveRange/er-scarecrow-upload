    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: Upload",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/er_scarecrow_upload/upload.py",
            "console": "integratedTerminal",
            "args": [
                "-s",
                "key.json",
                "-m",
                "mapping.json",
                "--upload",
                "--upload-directory",
                "test_gdriv",
                "--upload-local-directory=test",
                "--log-file",
                "./up.log"
            ]
        }
    ]
}